@model Libraary.Web.Models.Libraries.LibraryDetailsViewModel
@{
    ViewData["Title"] = "Home Page";
}


@if (User.IsInRole("Owner") || User.IsInRole("Librarian"))
{
    <partial name="/Views/Home/_OwnerHomePartial.cshtml" model="@Model" />
}
else
{
    <partial name="/Views/Home/_UserHomePartial.cshtml" />
}